<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
        <entry key="account.getCharactersForSelectionScreen">
            <![CDATA[
                { call p_account_getCharactersForSelection(?,?) }
            ]]>
        </entry>    
        <entry key="aura.loadAllAuras">
            <![CDATA[
                SELECT * FROM `aura` ORDER BY aur_aura_id
            ]]>
        </entry>    
        <entry key="equipment.addItem">
            <![CDATA[
                { call p_character_equipment_addItem(?,?,?,?,?,?,?) }
            ]]>
        </entry>                
        <entry key="equipment.deleteAllFromCharacter">
            <![CDATA[
                { call p_character_equipment_deleteAll(?,?) }
            ]]>
        </entry>        
        <entry key="equipment.getAllFromCharacter">
            <![CDATA[
                { call p_equipment_getAllCharacterItems(?,?) }
            ]]>
        </entry>
        <entry key="character.getComplementaryInfoForEnteringWorld">
            <![CDATA[
                { call p_character_getComplementaryInfo(?,?) }
            ]]>
        </entry>       
        <entry key="character.getAllInfoForEnteringWorld">
            <![CDATA[
                { call p_character_getAllInfo(?,?) }
            ]]>
        </entry>
        <entry key="character.createNew">
            <![CDATA[
                { call p_character_createNew(?,?,?,?,?,?) }
            ]]>
        </entry>
        <entry key="characterInventory.addItem">
            <![CDATA[
                { call p_character_inventory_addItem(?,?,?,?,?,?,?) }
            ]]>
        </entry>
        <entry key="characterInventory.removeAll">
            <![CDATA[
                { call p_character_inventory_removeAll(?,?) }
            ]]>
        </entry>
        <entry key="characterInventory.getAllItems">
            <![CDATA[
                { call p_character_inventory_getAllItems(?,?) }
            ]]>
        </entry>
        <entry key="characterSkillBook.getAllSkills">
            <![CDATA[
                { call p_character_skillbook_getSkills(?,?) }
            ]]>
        </entry>                                               
        <entry key="world.getAllWorldMapsOrderedById">
            <![CDATA[
                SELECT * FROM `world` ORDER BY 1
            ]]>
        </entry>     
        <entry key="itemData.getAllItemDataOrderedByItenId">
            <![CDATA[
                SELECT * FROM `item_data` ORDER BY idt_itn_cod_item
            ]]>
        </entry>                       
</properties>


